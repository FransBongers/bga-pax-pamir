{
  "name": "paxpamireditiontwo",
  "version": "1.0.0",
  "description": "Pax Pamir 2nd edition for BGA",
  "main": "paxpamireditiontwo.js",
  "scripts": {
    "build:ts": "tsc",
    "build:scss": "sass --no-source-map src/paxpamireditiontwo.scss paxpamireditiontwo.css",
    "watch:ts": "tsc --watch",
    "watch:scss": "sass --watch src/paxpamireditiontwo.scss paxpamireditiontwo.css",
    "watch": "npm run watch:ts && npm run watch:scss",
    "build": "npm run build:ts && npm run build:scss"
  },
  "author": "yourname",
  "license": "MIT",
  "devDependencies": {
    "sass": "^1.58.3",
    "typescript": "^4.1.3"
  }
}
