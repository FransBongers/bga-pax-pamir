{
  "name": "paxpamir",
  "version": "1.0.0",
  "description": "Pax Pamir 2nd edition for BGA",
  "main": "paxpamir.js",
  "scripts": {
    "build:ts": "tsc",
    "build:scss": "sass --no-source-map src/paxpamir.scss paxpamir.css",
    "watch:ts": "tsc --watch",
    "watch:scss": "sass --watch src/paxpamir.scss paxpamir.css",
    "watch": "npm run watch:ts && npm run watch:scss",
    "build": "npm run build:ts && npm run build:scss"
  },
  "author": "Frans Bongers",
  "license": "MIT",
  "dependencies": {
    "bga-animations": "thoun/bga-animations#1.1.2",
    "bga-cards": "thoun/bga-cards#3.1.1"
  },
  "devDependencies": {
    "sass": "^1.58.3",
    "typescript": "^4.1.3"
  }
}
